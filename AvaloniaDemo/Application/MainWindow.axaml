<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:bindings="clr-namespace:Application.Bindings"
        xmlns:compiledBindings="clr-namespace:Application.CompiledBindings"
        xmlns:styling="clr-namespace:Application.Styling"
        xmlns:application="clr-namespace:Application"
        xmlns:enumerables="clr-namespace:Application.Enumerables"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Application.MainWindow"
        Title="Application">
    <Grid>
        <Grid.ColumnDefinitions>*,*,*,*,*,*,*</Grid.ColumnDefinitions>
        <bindings:Bindings Grid.Column="0" />
        <!--<compiledBindings:CompiledBindings Grid.Column="1" />
        <compiledBindings:CompiledBindingsWithFlag Grid.Column="2" />-->
        <styling:Styling Grid.Column="3" />
        <application:Grids Grid.Column="4" />
        <enumerables:Enumerables Grid.Column="5" />
        <compiledBindings:CompiledBindingsConvertible Grid.Column="6" />
    </Grid>
</Window>